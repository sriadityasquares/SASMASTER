
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <title>@ViewBag.title</title>
    <link rel="icon" href="~/favicons/cropped-DiversityTraxLogo-icon-1-32x32.png" sizes="32x32">
    <link rel="icon" href="~/favicons/cropped-DiversityTraxLogo-icon-1-192x192.png" sizes="192x192">
    <link rel="apple-touch-icon-precomposed" href="~/favicons/cropped-DiversityTraxLogo-icon-1-180x180.png">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <link href="~/fonts/fontawesome-free-5.0.1/web-fonts-with-css/css/fontawesome-all.min.css" rel="stylesheet" />
</head>
<body>
    <div id="loadingImage" class="k-loading-image hidden"><div id="loader"></div></div>
    <div id="wrapper">
        <!-- start header -->
        <header>
            <div class="navbar navbar-default dashboard-wrap">
                <div class="container-fluid">
                    <div class="main-logo">
                        <div class="navbar-header">
                            <a class="navbar-brand" href="/"><img src="" alt="" /></a>
                        </div>
                    </div>
                    <div class="main-navmenu">
                        
                    </div>
                    <div class="right-userdiv icon">
                        <!-- Login Details -->
                        @*@Html.Partial("~/Views/Shared/_LoginPartial.cshtml")*@
                    </div>
                </div>
            </div>
        </header>
        
    </div>
    <!-- footer -->
    <footer class="footer">
        <div class="container">
            <div class=" col-md-12 row text-center">
                <div>&copy; @DateTime.UtcNow.Year | <a href="/Home/PrivacyPolicy" target="_blank">Privacy Policy</a> / <a href="/Home/TermsOfUse" target="_blank">Terms of Use</a></div>
                <!---Help Section START--->
                @if (User.Identity.IsAuthenticated)
                {
                    <div id="helpWrapper" class="help-button-wrapper">
                        <a class="help-button" href="/Manage/usersettings#tab_container4">
                            <span><i class="fa fa-question" aria-hidden="true"></i></span>
                        </a>
                    </div>
                }
                <!---Help Section END--->
            </div>
        </div>
    </footer>
</body>
</html>
